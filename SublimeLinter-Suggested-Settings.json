{
    "sublimelinter": true,
    "javascript_linter": "jshint",
    "jshint_options": {
        "white": false,
        "indent": 4,
        "onecase": false,
        "onevar": false,
        "immed": true,
        "debug": false,
        "evil": false,
        "strict": false,
        "multistr": false,
        "laxbreak": false,
        "globalstrict": false,
        "supernew": false,
        "laxcomma": false,
        "asi": false,
        "es5": false,
        "scripturl": false,
        "withstmt": false,
        "bitwise": true,
        "eqeqeq": true,
        "shadow": false,
        "expr": false,
        "noarg": true,
        "newcap": true,
        "forin": true,
        "regexdash": false,
        "node": true,
        "eqnull": false,
        "browser": true,
        "iterator": false,
        "undef": true,
        "latedef": true,
        "nonstandard": false,
        "trailing": true,
        "jquery": true,
        "loopfunc": false,
        "boss": false,
        "nonew": true,
        "funcscope": false,
        "regexp": true,
        "lastsemic": false,
        "smarttabs": true,
        "devel": false,
        "esnext": false,
        "sub": false,
        "curly": false,
        "prototypejs": false,
        "proto": false,
        "plusplus": false,
        "noempty": false,
        "predef": [
            "confirm",
            "alert",
            "jasmine",
            "spyOn",
            "it",
            "console",
            "describe",
            "expect",
            "beforeEach",
            "waits",
            "waitsFor",
            "runs",
            "chrome",
            "Sprite",
            "Class",
            "Canvas"]
    }
}